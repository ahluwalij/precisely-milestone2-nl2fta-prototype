{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "npx ng",
    "start": "AUTH_PORT=3001 node server/auth-server.js & npx ng serve --host 0.0.0.0 --proxy-config proxy.conf.js",
    "start:auth": "node server/auth-server.js",
    "start:local": "npx ng serve",
    "build": "npx ng build --configuration production",
    "test": "npx ng test --watch=false --browsers=ChromeHeadless",
    "test:watch": "npx ng test",
    "test:coverage": "npx ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "npx ng lint",
    "lint:fix": "npx ng lint --fix",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:css:fix": "stylelint \"src/**/*.css\" --fix",
    "format": "prettier --write \"src/**/*.{ts,html,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/ssr": "^18.2.0",
    "@primeng/themes": "^18.0.0",
    "@types/papaparse": "^5.3.16",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "papaparse": "^5.5.3",
    "primeicons": "^7.0.0",
    "primeng": "^18.0.0",
    "node-forge": "^1.3.1",
    "rxjs": "~7.8.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular-eslint/builder": "^18.0.0",
    "@angular-eslint/eslint-plugin": "^18.0.0",
    "@angular-eslint/eslint-plugin-template": "^18.0.0",
    "@angular-eslint/template-parser": "^18.0.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.19.117",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vercel/node": "^5.3.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.2.0",
    "stylelint": "^16.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-order": "^6.0.0",
    "typescript": "~5.4.0"
  }
}
